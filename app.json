{
  "expo": {
    "userInterfaceStyle": "automatic",
    "name": "Inex",
    "slug": "inex-expenses",
    "version": "1.1.0",
    "icon": "./assets/icon.png",
    "backgroundColor": "#FFFFFF",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "scheme": "inexbudget",
    "ios": {
      "buildNumber": "1.1.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.sublimeapps.inexexpenses",
      "associatedDomains": ["applinks:inexbudget.onelink.me"],
      "infoPlist": {
        "CFBundleDevelopmentRegion": "en",
        "CFBundleLocalizations": ["en", "fr"],
        "CFBundleDisplayName": "Inex Expenses",
        "CFBundleShortVersionString": "1.1.0",
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        },
        "NSCameraUsageDescription": "This app needs access to the camera to allow you to take photos",
        "NSMicrophoneUsageDescription": "This app needs access to the microphone for recording audio",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library for you to choose photos",
        "NSUserTrackingUsageDescription": "Allowing app tracking will improve the customization of ads",
        "UIApplicationExitsOnSuspend": true
      }
    },
    "android": {
      "versionCode": 2
    },
    "extra": {
      "eas": {
        "projectId": "fdbc9fca-630f-48f3-a650-67eaf3826dc0"
      }
    },
    "plugins": [
      ["react-native-appsflyer", {}],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "Allowing app tracking will improve the customization of ads"
        }
      ]
    ]
  }
}
